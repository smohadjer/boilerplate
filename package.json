{
	"name": "boilerplate",
	"version": "1.0.1",
	"description": "Boilerplate for Web projects",
	"repository": {
		"type": "git",
		"url": "https://github.com/smohadjer/boilerplate"
	},
	"license": "MIT",
	"scripts": {
		"precompile": "node node_modules/build/bin/clean.js",
		"compile": "node node_modules/build/bin/compile.js",
		"copy": "node node_modules/build/bin/copy.js",
		"sass": "node node_modules/build/bin/sassTocss",
		"hbs": "node node_modules/build/bin/hbs.js",
		"watch": "node node_modules/build/bin/watch.js",
		"server": "node node_modules/build/bin/server.js",
		"serve": "npm run compile && npm run copy && npm run sass && npm run hbs",
		"start": "npm run serve && npm run watch & npm run server -- public 5000",
		"build": "npm run serve",
		"start-prod": "npm run build && npm run server -- dist 3001"
	},
	"devDependencies": {
		"build": "git://github.com/smohadjer/build.git#semver:^1.0.7"
	},
	"dependencies": {
		"hamburgers": "0.9.3",
		"helpers": "git+https://github.com/smohadjer/helpers.git",
		"swiper": "^10.0.4"
	},
	"engines": {
		"node": ">=16.13.0"
	}
}
